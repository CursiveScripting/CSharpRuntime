<Processes>
<Process name="Test.MorningRoutine">
	<Description>Runs a test morning routine sequence</Description>
	<Input type="person">Me</Input>
	<Input type="car">Car</Input>
	<Output type="integer">My age</Output>
	
	<Steps firstStep="getAge">
		<Step name="getAge" process="get">
			<FixedInput name="property" value="Age" />
			<MapInput name="object" source="Me" />
			<MapOutput name="value" destination="age" />
			<ReturnPaths default="breakfastAgeCheck" />
		</Step>
		<Step name="breakfastAgeCheck" process="compare">
			<MapInput name="value1" source="age" />
			<FixedInput name="value2" value="10" />
			<ReturnPaths default="makeBreakfast">
				<Path name="less" value="demandBreakfast" />
			</ReturnPaths>
		</Step>
		<Step name="makeBreakfast" process="print">
			<FixedInput name="message" value="Making breakfast..." />
			<ReturnPaths default="eatBreakfast" />
		</Step>
		<Step name="demandBreakfast" process="print">
			<FixedInput name="message" value="Demanding breakfast... (young enough to get away with this)" />
			<ReturnPaths default="eatBreakfast" />
		</Step>
		<Step name="eatBreakfast" process="print">
			<FixedInput name="message" value="Eating breakfast..." />
			<ReturnPaths default="getReady" />
		</Step>
		<Step name="getReady" process="print">
			<FixedInput name="message" value="Getting dressed etc..." />
			<ReturnPaths default="checkDay" />
		</Step>
		<Step name="checkDay" process="print">
			<ReturnPaths default="getInCar">
				<Path name="Saturday" value="done" />
				<Path name="Sunday" value="done" />
			</ReturnPaths>
		</Step>
		<Step name="getInCar" process="print">
			<FixedInput name="message" value="Getting in the car..." />
			<ReturnPaths default="done" />
		</Step>
		<EndStep name="done">
			<MapInput name="age" destination="My age" />
		</EndStep>
	</Steps>
</Process>
</Processes>